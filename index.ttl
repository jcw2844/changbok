@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix schema: <https://schema.org/> .
@prefix edu: <http://library.school.kr/schema/education/> .
@prefix ex: <http://example.org/history/> .

# 1. 인물 정보: 엄복동 (주어)
ex:UmBokDong a foaf:Person ;
    foaf:name "엄복동" ;
    schema:birthDate "1892" ;
    schema:deathDate "1951" ;
    edu:occupation "자전거 선수" ;
    ex:nickname "자전차왕" ;
    ex:signatureMove "엉덩이 들기(Rising)" ; # 주특기
    ex:symbolism "일제강점기 민족의 자긍심 고취" ; # 상징성
    dc:description "하늘에는 안창남, 땅에는 엄복동이라 불린 근대 스포츠 영웅" ;
    
    # 관계 설정: 특정 자전거를 사용함
    ex:usedVehicle ex:UmBokDongBicycle ;
    
    # 관계 설정: 특정 대회의 우승자임
    ex:winnerOf ex:AllChosunBicycleRace .

# 2. 관련 사물: 엄복동의 자전거 (등록문화재 제466호)
ex:UmBokDongBicycle a schema:Product ;
    dc:title "엄복동 자전거" ;
    schema:manufacturer "Rudge-Whitworth (영국)" ;
    schema:category "등록문화재 제466호" ;
    dc:description "엄복동 선수가 경기 시 직접 사용한 영국제 경주용 자전거" .

# 3. 주요 사건: 전조선 자전차 경기대회
ex:AllChosunBicycleRace a schema:Event ;
    dc:title "전조선 자전차 경기대회" ;
    schema:location "경성(서울)" ;
    schema:startDate "1913" ;
    dc:description "엄복동이 일본 선수들을 물리치고 첫 우승을 차지한 역사적 대회" .